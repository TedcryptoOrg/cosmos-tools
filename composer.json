{
    "type": "project",
    "name": "tedcrypto/cosmos-tools",
    "description": "Mixed cosmos tools for all cosmos validators and delegators",
    "license": "proprietary",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-iconv": "*",
        "doctrine/annotations": "^2.0",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.13",
        "eluceo/ical": "^2.11",
        "guzzlehttp/guzzle": "^7.0",
        "jms/serializer-bundle": "^5.3",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.13",
        "sensio/framework-extra-bundle": "^6.1",
        "sentry/sentry-symfony": "^4.4",
        "symfony/asset": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/doctrine-messenger": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/filesystem": "6.4.*",
        "symfony/flex": "^2",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/http-client": "6.4.*",
        "symfony/intl": "6.4.*",
        "symfony/mailer": "6.4.*",
        "symfony/messenger": "6.4.*",
        "symfony/mime": "6.4.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "6.4.*",
        "symfony/process": "6.4.*",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/proxy-manager-bridge": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.*",
        "symfony/serializer": "6.4.*",
        "symfony/string": "6.4.*",
        "symfony/translation": "6.4.*",
        "symfony/twig-bundle": "6.4.*",
        "symfony/validator": "6.4.*",
        "symfony/web-link": "6.4.*",
        "symfony/webpack-encore-bundle": "^2.0",
        "symfony/yaml": "6.4.*",
        "tedcrypto/cosmos-accounts": "^1.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0"
    },
    "require-dev": {
        "hautelook/alice-bundle": "^2.11",
        "jangregor/phpstan-prophecy": "^1.0",
        "php-cs-fixer/shim": "^3.23",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-beberlei-assert": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpstan/phpstan-symfony": "^1.3",
        "phpunit/phpunit": "^11.0",
        "rector/rector": "^0.18.0",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "6.4.*",
        "symfony/debug-bundle": "6.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/stopwatch": "6.4.*",
        "symfony/web-profiler-bundle": "6.4.*"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "php-http/discovery": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*",
        "symfony/polyfill-php82": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "code-style:fix": [
            "PHP_CS_FIXER_FUTURE_MODE=1 php-cs-fixer fix --diff --ansi --using-cache=no"
        ],
        "code-style:check": [
            "PHP_CS_FIXER_FUTURE_MODE=1 php-cs-fixer fix --dry-run --diff --ansi --using-cache=no"
        ],
        "phpstan": [
            "./bin/console cache:warmup --env=test",
            "phpstan analyse --memory-limit=-1 --ansi --no-progress"
        ],
        "phpstan:github-action": [
            "./bin/console cache:warmup --env=test",
            "phpstan analyse --memory-limit=-1 public/index.php config/ bin/ src/ tests/ --ansi --no-progress --error-format=github"
        ],
        "rector:fix": [
            "rector --ansi --no-progress-bar"
        ],
        "rector:check": [
            "rector --dry-run --ansi --no-progress-bar"
        ],
        "static-analysis": [
            "@composer validate",
            "@code-style:check",
            "@phpstan",
            "@rector:check"
        ],
        "phpunit:unit": [
            "phpunit --colors --testsuite=unit --do-not-cache-result"
        ],
        "phpunit:integration": [
            "phpunit --colors --testsuite=integration --do-not-cache-result"
        ],
        "phpunit:functional": [
            "phpunit --colors --testsuite=functional --do-not-cache-result"
        ],
        "tests:unit": [
            "@phpunit:unit"
        ],
        "tests:integration": [
            "@phpunit:integration"
        ],
        "tests:functional": [
            "@phpunit:functional"
        ],
        "tests:prepare-db": [
            "php bin/console doctrine:database:drop --force --env=test --if-exists",
            "php bin/console doctrine:database:create --env=test",
            "php bin/console doctrine:migrations:migrate --no-interaction --env=test"
        ],
        "tests": [
            "@tests:unit",
            "@tests:prepare-db",
            "@tests:integration",
            "@tests:functional"
        ],
        "ci": [
            "@static-analysis",
            "@tests"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "6.3.*"
        }
    }
}
